
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"> 
    <title>Index</title>

    <link href="~/Extjs/resources/theme-classic-all.css" rel="stylesheet" />
    <link href="~/Extjs/ux/classic/resources/ux-all.css" rel="stylesheet" />
    <link href="~/Content/upload.css" rel="stylesheet" />
    <link href="~/Content/icon.css" rel="stylesheet" />
</head>
<body>

    <script type="text/javascript" src="~/Extjs/ext-all-debug.js"></script>
    <script src="~/Extjs/theme-classic.js"></script>
    <script src="~/Extjs/ux/ux.js"></script>
    <script src="~/xf/plugin/exporter/downloadify.min.js"></script>
    <script type="text/javascript">
        Ext.Loader.setConfig({
            enable: true,
            paths: {
                "xf.core": "/xf",
                "xf": "/app",
                "xf.view": "/app/view",
                "xf.controller": "/app/controller",
                "xf.model": "/app/model",
                "Ext.ux.upload": "/xf/plugin/upload",
                "Ext.ux.exporter": "/xf/plugin/exporter"
            }
        });
    </script>

    <script type="text/javascript">

        Ext.onReady(function () {
            Ext.toast("欢迎使用EAP", "欢迎", "t");
            Ext.application("xf.core.Application");
        });

    </script>
    @Scripts.Render("~/xf")

    @Scripts.Render("~/app/model")

</body>
</html>
